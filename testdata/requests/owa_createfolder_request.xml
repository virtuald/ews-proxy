    <soap:Header>
        <t:RequestServerVersion Version="Exchange2013"/>
        <t:TimeZoneContext>
            <t:TimeZoneDefinition Id="Eastern Standard Time"/>
        </t:TimeZoneContext>
    </soap:Header>
    <soap:Body>
        <m:CreateFolder>
            <m:ParentFolderId>
                <t:DistinguishedFolderId Id="msgfolderroot"/>
            </m:ParentFolderId>
            <m:Folders>
                <t:Folder>
                    <t:DisplayName>test</t:DisplayName>
                    <t:FolderClass>IPF.Note</t:FolderClass>
                </t:Folder>
            </m:Folders>
        </m:CreateFolder>
    </soap:Body>
